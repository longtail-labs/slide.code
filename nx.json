{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "targetDefaults": {
    "build": {
      "configurations": {
        "development": {},
        "beta": {},
        "production": {}
      },
      "dependsOn": ["^build"],
      "outputs": ["{projectRoot}/dist/**"],
      "cache": true
    },
    "compile": {
      "dependsOn": ["^build", "^compile"],
      "outputs": ["{projectRoot}/dist/**"],
      "cache": true
    },
    "lint": {
      "cache": true
    },
    "test": {
      "outputs": ["{projectRoot}/coverage/**"],
      "cache": true
    },
    "typecheck": {
      "cache": true
    },
    "dev": {
      "cache": false
    },
    "stub": {
      "cache": true
    }
  },
  "defaultBase": "master"
}
